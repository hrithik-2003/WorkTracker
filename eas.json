{
    "cli": {
        "version": ">= 16.0.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EAS_BUILD_NPM_INSTALL_COMMAND": "npm install"
            }
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            }
        },
        "production": {
            "autoIncrement": true
        }
    },
    "submit": {
        "production": {
            "android": {
                "serviceAccountKeyPath": "./google-services.json",
                "track": "internal"
            }
        }
    }
}